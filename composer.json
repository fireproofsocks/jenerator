{
    "name": "jenerator/jenerator",
    "description": "Generates JSON values, files, and examples from JSON Schema definitions for use as seed data, sample API responses, etc.",
    "type": "library",

    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "^3.3",
        "pimple/pimple": "^3.1.0",
        "fzaninotto/faker": "^1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.8",
        "mockery/mockery": "^0.9.7"
    },
    "autoload": {
        "psr-4": {
            "Jenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeneratorTest\\": "tests/"
        }
    }
}
